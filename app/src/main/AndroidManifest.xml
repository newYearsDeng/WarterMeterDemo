        <activity
            android:name=".activity.TakePhotoActivity_HP"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TaskBuildUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TaskMaintainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.TaskMeterActivity" />
        <activity
            android:name=".activity.TaskRepairActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TaskScanBuildUserActivity"
